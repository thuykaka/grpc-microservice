{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "compile-proto": "protoc --plugin=\"./node_modules/.bin/protoc-gen-ts.cmd\" --plugin=protoc-gen-grpc=\"C:/Users/thuy.lv/Desktop/web-starting/grpc-microservice/hero/node_modules/.bin/grpc_tools_node_protoc_plugin.cmd\" --js_out=\"import_style=commonjs,binary:./generated\" --ts_out=\"service=grpc-node,mode=grpc-js:./generated\" --grpc_out=\"grpc_js:./generated\" src/proto/hero.proto",
    "protoc": "protoc --plugin=\"./node_modules/.bin/protoc-gen-ts_proto.cmd\" -I=./src/proto --ts_proto_out=./src/interfaces ./src/proto/hero.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=addNestjsRestParameter=true"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.4.2",
    "@nestjs/common": "8.1.2",
    "@nestjs/core": "8.1.2",
    "@nestjs/microservices": "8.1.2",
    "@nestjs/platform-express": "8.1.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.4",
    "@nestjs/testing": "8.1.2",
    "@types/express": "4.17.13",
    "@types/node": "16.11.6",
    "@types/supertest": "2.0.11",
    "@types/ws": "7.4.7",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "ts-proto": "1.150.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.3.5"
  }
}